{
  "name": "fdk-cms-service",
  "private": true,
  "version": "0.1.0",
  "description": "FDK CMS Service built with Strapi",
  "license": "Apache-2.0",
  "keywords": [
    "strapi",
    "cms",
    "typescript",
    "nodejs"
  ],
  "author": "FDK Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fdk-cms-service.git"
  },
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "pre-commit": "yarn lint && yarn type-check && yarn format:check",
    "clean": "rm -rf dist build .cache .tmp",
    "dev": "yarn develop"
  },
  "devDependencies": {
    "@babel/runtime": "^7.28.3",
    "@koa/router": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0"
  },
  "dependencies": {
    "@strapi/icons": "^1.19.0",
    "@strapi/plugin-graphql": "^5.23.1",
    "@strapi/plugin-users-permissions": "^5.23.1",
    "@strapi/provider-email-nodemailer": "5.23.1",
    "@strapi/strapi": "^5.23.1",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.1.19"
  },
  "resolutions": {
    "brace-expansion": "^1.1.12",
    "esbuild": "^0.25.0",
    "form-data": "^4.0.4",
    "formidable": "^2.1.3",
    "tmp": "^0.2.4",
    "koa": "^2.16.2",
    "sha.js": "^2.4.12"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
